    static int CalculateCombinations(int n, int m)
    {
        // 이항 계수를 이용하여 조합을 계산
        int[,] dp = new int[n + 1, m + 1];

        for (int i = 0; i <= m; i++)
        {
            dp[0, i] = 1;
        }

        for (int i = 1; i <= n; i++)
        {
            for (int j = i; j <= m; j++)
            {
                dp[i, j] = dp[i, j - 1] + dp[i - 1, j - 1];
            }
        }

        return dp[n, m];
    }
